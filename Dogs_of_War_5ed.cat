    <selectionEntry id="394b-5ea6-a641-feeb" name="Lucrezzia Belladonna" hidden="false" collective="false" import="true" type="unit">
      <categoryLinks>
        <categoryLink id="84d2-f94a-447e-c5fb" name="Characters" hidden="false" targetId="48f1-4778-a9db-cde7" primary="true"/>
