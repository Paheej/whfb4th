            <selectionEntry type="upgrade" import="true" name="Compass of Meteoric Silver" hidden="false" id="6cf7-16fd-d34e-f8c5" defaultAmount="1">
              <costs>
                <cost name="pts" typeId="eaa7-6800-e651-8bea" value="10"/>
